@model IEnumerable<superHero.Models.heroInfo>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Heros Made already ";
}
<h1>All Heros</h1>
<table class="table">
    <thead>
    <th>Hero Name</th>
    <th>Origin</th>
    </thead>
    <tbody>

        @foreach (var comp in Model)
        {
            <tr>
                <td>
                    @comp.name
                </td>
                <td>@comp.origin</td>
            </tr>
           
        }
    </tbody>

</table>
